<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--========================= Blue Rounded Glitter-Glass Wall =============================-->

    <ThingDef ParentName="SW_RoundedGlitterGlassWallBase">
        <defName>SW_BlueRoundedGlitterGlassWall</defName>
        <label>blue rounded glitter glass wall</label>
        <graphicData>
            <texPath>StevesWalls/Things/Building/Linked/SW_RoundedGlitterGlassWall/Blue</texPath>
        </graphicData>
        <comps>
            <li Class="CompProperties_Flickable"/>
            <li Class="CompProperties_Power">
                <compClass>CompPowerTrader</compClass>
                <basePowerConsumption>1</basePowerConsumption>
                <transmitsPower>true</transmitsPower>
            </li>
            <li Class="CompProperties_Glower">
                <glowRadius>3</glowRadius>
                <glowColor>(0,183,255)</glowColor>
            </li>
        </comps>
    </ThingDef>

